<%- include("partials/header") -%>

<h1>Compose</h1>
<form action="/compose" method="post">
    <div class="form-group">
    <label for="postTitle">Title</label>
    <input class="form-control" type="text" id="postTitle" name="postTitle">
    <label for="postBody">Post</label>
    <textarea class="form-control" id="postBody" name="postBody" cols="75" rows="5"></textarea>
    <button class="btn btn-primary" type="submit" name="submit">Publish</button>
</div>
</form>

<%- include("partials/footer") -%>